{
    "database":
    {
        "connection": "mysql+pymysql://neutron:secret@openstack_mariadb.openstack-management-net/neutron"
    },
    "DEFAULT":
    {
        "core_plugin": "ml2",
        "service_plugins": "",
        "transport_url": "rabbit://guest:guest@openstack_rabbitmq.openstack-management-net",
        "auth_strategy": "keystone",
        "notify_nova_on_port_status_changes": "true",
        "notify_nova_on_port_data_changes": "true"
    },
    "keystone_authtoken":
    {
        "auth_uri": "http://openstack_keystone.openstack-management-net:5000",
        "auth_url": "http://openstack_keystone.openstack-management-net:35357",
        "memcached_servers": "openstack_memcached:11211",
        "auth_type": "password",
        "project_domain_name": "Default",
        "user_domain_name": "Default",
        "project_name": "service",
        "username": "neutron",
        "password": "secret"
    },
    "nova":
    {
        "auth_uri": "http://openstack_keystone.openstack-management-net:5000",
        "auth_url": "http://openstack_keystone.openstack-management-net:35357",
        "memcached_servers": "openstack_memcached.openstack-management-net:11211",
        "auth_type": "password",
        "project_domain_name": "Default",
        "user_domain_name": "Default",
        "project_name": "service",
        "username": "nova",
        "password": "secret"
    },
    "oslo_concurrency":
    {
        "lock_path": "/var/lib/neutron/tmp"
    },
    "ml2":
    {
        "type_drivers": "flat,vlan",
        "tenant_network_types": "",
        "mechanism_drivers": "linuxbridge",
        "extension_drivers": "port_security"
    },
    "ml2_type_flat":
    {
        "flat_networks": "provider"
    },
    "securitygroup":
    {
        "enable_ipset": "true"
    }
}